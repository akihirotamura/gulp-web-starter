{
  "name": "myproject",
  "version": "0.1.0",
  "authors": [
    "Yutaro <yutaro@studio-rubbish.com>"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "*",
    "foundation": "*",
    "bxSlider": "*"
  },
  "overrides": {
    "jquery": {
      "ignore": true
    },
    "foundation": {
      "dependencies": null,
      "main": [
        "**/foundation.js",
        "scss/foundation/components/_*.scss",
        "scss/foundation/_*.scss"
      ],
      "normalize": {
        "../src/js/lib": "*.js",
        "../src/scss/core/foundation": ["_functions.scss", "_settings.scss"],
        "../src/scss/core/foundation/components": ["!_functions.scss", "!_settings.scss", "_*.scss"]
      }
    },
    "bxSlider": {
      "dependencies": null,
      "main": [
        "**/jquery.bxslider.min.js",
        "**/jquery.bxslider.css",
        "**/bx_loader.gif",
        "**/controls.png"
      ],
      "normalize": {
        "../shared/images": ["*.gif", "*.png"],
        "../shared/js": "**/*.js",
        "../bower_components/tmp/css": "**/*.css"
      }
    }
  }
}
