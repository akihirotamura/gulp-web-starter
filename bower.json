{
  "name": "myproject",
  "version": "0.1.0",
  "authors": [
    "Yutaro <yutaro@studio-rubbish.com>"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "*",
    "foundation": "*",
    "bxSlider": "*"
  },
  "overrides": {
    "jquery": {
      "ignore": true
    },
    "foundation": {
      "dependencies": null,
      "main": [
        "**/foundation.js"
      ],
      "normalize": {
        "../src/js/lib": "*.js"
      }
    },
    "bxSlider": {
      "dependencies": null,
      "main": [
        "**/jquery.bxslider.min.js",
        "**/jquery.bxslider.css",
        "**/bx_loader.gif",
        "**/controls.png"
      ],
      "normalize": {
        "../shared/images": ["*.gif", "*.png"],
        "../src/js/lib": "**/*.js",
        "../src/scss/module": "**/*.css"
      }
    }
  }
}
